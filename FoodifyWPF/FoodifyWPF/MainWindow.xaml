<Window x:Class="FoodifyWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FoodifyWPF"
        mc:Ignorable="d"
        Title="Foodify Karbantartó" Height="450" Width="800">
    <Grid>
        <Menu>
            <MenuItem Header="Bejelentkezés" Click="OpenLoginWindow_Click"/>
            <MenuItem Header="Felhasználók" IsEnabled="False">
                <MenuItem Header="Felhasználók listázása"/>
                <MenuItem Header="Felhasználók rögzítése"/>
                <MenuItem Header="Felhasználók törlése"/>
            </MenuItem>
        </Menu>
    </Grid>
</Window>
